#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/melissaspiegel/projects/node_modules/.pnpm/vite@5.4.18_@types+node@22.14.0_sass@1.86.3_terser@5.39.0/node_modules/vite/bin/node_modules:/Users/melissaspiegel/projects/node_modules/.pnpm/vite@5.4.18_@types+node@22.14.0_sass@1.86.3_terser@5.39.0/node_modules/vite/node_modules:/Users/melissaspiegel/projects/node_modules/.pnpm/vite@5.4.18_@types+node@22.14.0_sass@1.86.3_terser@5.39.0/node_modules:/Users/melissaspiegel/projects/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/melissaspiegel/projects/node_modules/.pnpm/vite@5.4.18_@types+node@22.14.0_sass@1.86.3_terser@5.39.0/node_modules/vite/bin/node_modules:/Users/melissaspiegel/projects/node_modules/.pnpm/vite@5.4.18_@types+node@22.14.0_sass@1.86.3_terser@5.39.0/node_modules/vite/node_modules:/Users/melissaspiegel/projects/node_modules/.pnpm/vite@5.4.18_@types+node@22.14.0_sass@1.86.3_terser@5.39.0/node_modules:/Users/melissaspiegel/projects/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/vite@5.4.18_@types+node@22.14.0_sass@1.86.3_terser@5.39.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/vite@5.4.18_@types+node@22.14.0_sass@1.86.3_terser@5.39.0/node_modules/vite/bin/vite.js" "$@"
fi
